    <TIER ANNOTATOR="RAÚL QUIETA" LINGUISTIC_TYPE_REF="TRADUCCIÓN" PARENT_REF="palabra en a'ingae (ortografía nueva)" PARTICIPANT="SANDRA QUIETA" TIER_ID="frase en Español">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a35" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>Mi nombre es Sandra Quieta, tengo 38 años, y soy de comuna Duvûno</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a36" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>Voy a coversar sobre como se hacer o elaborar una batea</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a37" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>si, asi se dice</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a38" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>a bueno; ir al bosque buscar la madera y cortar la pampa de arbol</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a39" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>se trae a la casa y a veces se camina hasta 3 horas; se deja que se seque y de ahi se hace la batea</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a40" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>no, cortan de cedro y canelo para hacer la batea</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a41" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>dependiendo, se hace de difentes formas; hay algunos que hacen el mismo dia, sin dejar secar. Hay otros que hacen pasando o secando de 1 a 2 dias; de ahi hacen la batea</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a42" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>en un solo dia; si usted en realidad sabe hacer, en un solo dia se hace</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a43" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>si ni sabes como hacer, te demoras de 2 a 3 dias en elaborar  eso</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a44" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE>se hace con azuela y la parte exterior con hacha se saca; o si tiene motosierra con motosierra o si no con hacha se saca la parte exterior de la batea</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a45" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>si, al terminar de hacer todo; ya sacado la parte interna, hay que quemar </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a46" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE>para minar</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a47" ANNOTATION_REF="a25">
                <ANNOTATION_VALUE>para eso no mas; la batea sirve para lavar oro</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a48" ANNOTATION_REF="a26">
                <ANNOTATION_VALUE>no, solo eso no mas</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
