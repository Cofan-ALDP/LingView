        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a23" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>Buenos días  nos encontramos  en la comunidad Sinangoe </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a24" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>donde nos encontramos con el compañero Cesilio Alvarado el nos va a contar un poco sobre su infacía </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a25" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>ya muchas gracías</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="LEIDY QUENAMA" LINGUISTIC_TYPE_REF="TRADUCCION"
        PARENT_REF="CA-CON" PARTICIPANT="CESILIO ALVARADO" TIER_ID="CA-SPA">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a26" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>lugar</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a27" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>ya, yo antes cuando era niño  no podia ir de casería</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a28" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>vivía con mis abuelos </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a29" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>mi abuelito un día salio de casería, y yo me quede  con mi abuelita en la orilla del río San Miguel</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a30" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>y en la  noche escuchamos que  vajaba  una canoa de motor por el rio, donde mi  abuelita  sintio miedo y me mijo</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a31" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>creo que vienen los guerrilleros</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a32" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>biendo que se acercabam huimos de la casa</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a33" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>pero  en la noche no se veia ademas no teníamos linterna </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a34" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>pero salimos  del  lugar</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a35" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>y nos metimos sobre un hueco</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a36" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>me cái sobre el y me puse a llorar</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a37" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>y me dijo mi abuelita no llores aguantese</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a38" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>luego dice han sido otros</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a39" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>luego salimos del lugar  y nos fuimos </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a40" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>estabamos en un lugar pantanoso</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a41" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>pensabamos que eran los guerrilleros</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a42" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE>y  an sido  los kichwas</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a43" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE>que se hivan a econtrarse con la novia, y nosotros asustados degana</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a44" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE>eso me paso cuando era niño.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
